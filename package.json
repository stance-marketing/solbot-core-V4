{
  "name": "solana-trading-dashboard",
  "version": "1.0.0",
  "description": "Professional Solana Trading Bot Dashboard",
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 12000",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "server": "node server-production.cjs",
    "start": "node server-production.cjs",
    "start-backend": "node server-production.cjs",
    "start-all": "npm run build && concurrently \"npm run start-backend\" \"npm run preview -- --host 0.0.0.0 --port 3000\"",
    "vercel-build": "npm run build",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.0.1",
    "@solana/spl-token": "^0.4.8",
    "@solana/web3.js": "^1.95.4",
    "axios": "^1.10.0",
    "bs58": "^6.0.0",
    "clsx": "^2.0.0",
    "cors": "^2.8.5",
    "date-fns": "^3.0.6",
    "date-fns-tz": "^3.0.0",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "framer-motion": "^10.16.16",
    "lucide-react": "^0.294.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.4.1",
    "react-redux": "^9.0.4",
    "react-router-dom": "^6.20.1",
    "recharts": "^2.8.0",
    "tailwind-merge": "^2.1.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "eslint": "^9.0.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}